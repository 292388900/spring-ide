      <!-- Target Definiton for latest snapshot Spring DM version -->	
      <target
            definition="targets/snapshot/spring-dm-snapshot.target"
            id="org.springframework.ide.eclipse.osgi.targetdefinition.snapshot"
            name="Spring Dynamic Modules for OSGi(tm) Service Platforms Snapshot">
      </target>
   </extension>

</plugin>
